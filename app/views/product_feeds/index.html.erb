<h1>Listing product_feeds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_feeds.each do |product_feed| %>
      <tr>
        <td><%= link_to 'Show', product_feed %></td>
        <td><%= link_to 'Edit', edit_product_feed_path(product_feed) %></td>
        <td><%= link_to 'Destroy', product_feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product feed', new_product_feed_path %>
